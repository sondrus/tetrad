{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": [
		"env.d.ts",
		"src/**/*",
		"src/**/*.vue"
, "i18n.ts"	],
	"exclude": [
		"src/**/__tests__/*"
	],
	"compilerOptions": {
		"target": "es2022",
		"lib": [
			"dom",
			"es2022"
		],
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	}
}